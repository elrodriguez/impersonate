
{
    "name": "elrod/laravel-multitenancy-impersonate",
    "description": "Laravel Multitenancy Impersonation from landlord to tenant",
    "authors": [
        {
            "name": "elmer rodriguez",
            "email": "elrodriguez2423@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-multitenancy": "^2.3"
    },
    "autoload": {
        "psr-4":{
            "Elrod\\MultitenancyImpersonate\\": "src"
        }
    },
	"config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elrod\\MultitenancyImpersonate\\MultitenancyImpersonateServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
